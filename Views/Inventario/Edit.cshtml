@model ProyectoWeb.Models.TbInventario
    <div class="container">
        <div class="span12">
            <div class="widget">
                <div class="widget-header">
                    <h4>TbInventario</h4>
                    </div>
                        <div class="widget-content">
                            <form asp-action="Edit">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <input type="hidden" asp-for="IdInventario" />
                                <div class="form-group">
                                    <label asp-for="Nombre" class="control-label"></label>
                                    <input asp-for="Nombre" class="span10 form-control" />
                                    <span asp-validation-for="Nombre" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Matricula" class="control-label"></label>
                                    <input asp-for="Matricula" class="span10 form-control" />
                                    <span asp-validation-for="Matricula" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Vendedor" class="control-label"></label>
                                    <input asp-for="Vendedor" class="span10 form-control" />
                                    <span asp-validation-for="Vendedor" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Cantidad" class="control-label"></label>
                                    <input asp-for="Cantidad" class="span10 form-control" />
                                    <span asp-validation-for="Cantidad" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Empresa" class="control-label"></label>
                                    <select asp-for="Empresa" class="span10 form-control" asp-items="ViewBag.Empresas"></select>
                                    <span asp-validation-for="Empresa" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="ParaFactura" class="control-label"></label>
                                    <select asp-for="ParaFactura" class="span10 form-control" asp-items="ViewBag.ParaFactura"></select>
                                    <span asp-validation-for="ParaFactura" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <input type="submit" value="Save" class="btn btn-primary" />
                                </div>
                            </form>
                        </div>
                    </div>
            </div>
        </div>

                    <div>
                        <a asp-action="Index">Back to List</a>
                    </div>

